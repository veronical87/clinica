	<subDataset name="veterinaria" uuid="09309f4e-ca10-4004-876c-f32908bae6a3">
		<queryString>
			<![CDATA[SELECT
     vacunasxmascota.`fecha` AS vacunasxmascota_fecha,
     controlvacunas.`nombre` AS controlvacunas_nombre
FROM
     `controlvacunas` controlvacunas INNER JOIN `vacunasxmascota` vacunasxmascota ON controlvacunas.`id` = vacunasxmascota.`idvacuna`]]>
		</queryString>
		<field name="vacunasxmascota_fecha" class="java.lang.String"/>
		<field name="controlvacunas_nombre" class="java.lang.String"/>
	</subDataset>
