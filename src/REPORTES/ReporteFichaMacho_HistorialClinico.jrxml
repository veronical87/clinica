	<subDataset name="VACUNAS" uuid="77a79844-1134-49b8-81ca-024b1ff5a4ea">
		<parameter name="DNI" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="IDFICHA" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT DATE_FORMAT(vacunasxmascota.fecha,'%d/%m/%Y') AS fecha,controlvacunas.nombre FROM controlvacunas INNER JOIN vacunasxmascota ON controlvacunas.id=vacunasxmascota.idvacuna INNER JOIN historialclinico ON vacunasxmascota.idhistorial=historialclinico.id INNER JOIN fichamedica ON historialclinico.idficha=fichamedica.id INNER JOIN propietarios ON fichamedica.idpropietario=propietarios.id WHERE dni=$P{DNI} AND fichamedica.id=$P{IDFICHA}]]>
		</queryString>
		<field name="fecha" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
	</subDataset>
