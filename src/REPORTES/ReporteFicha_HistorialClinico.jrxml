			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT afecciones.nombre AS afeccion FROM afecciones INNER JOIN afeccionesxhistorialclinico ON afecciones.id=afeccionesxhistorialclinico.idafeccion INNER JOIN historialclinico ON afeccionesxhistorialclinico.idhistorial=historialclinico.id INNER JOIN fichamedica ON historialclinico.idficha=fichamedica.id WHERE historialclinico.idficha=$P{IDFICHA}]]>
		</queryString>
		<field name="afeccion" class="java.lang.String"/>
