	<subDataset name="REMEDIOS" uuid="93f21166-3536-4a5e-8d48-a145eb1eecb0">
		<parameter name="IDPROPIETARIO" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT categoriamedicamento.nombre AS CATEGORIA, medicamentos.nombre AS remedio FROM categoriamedicamento inner join medicamentos on categoriamedicamento.id=medicamentos.idcategoria INNER JOIN medicamentosxoperacion ON medicamentos.id=medicamentosxoperacion.idmedicamento WHERE medicamentosxoperacion.idoperacion=$P{IDPROPIETARIO}]]>
		</queryString>
		<field name="CATEGORIA" class="java.lang.String"/>
		<field name="remedio" class="java.lang.String"/>
	</subDataset>
	<subDataset name="SECCIONES" uuid="76315ae6-4267-4a01-a2ee-b18cd4224b37">
		<parameter name="IDOPERACION" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT zonas.nombre AS zona,secciones.nombre FROM zonas INNER JOIN secciones ON zonas.id=secciones.idzona INNER JOIN seccionesaoperar ON secciones.id=seccionesaoperar.idseccion INNER JOIN operaciones ON seccionesaoperar.idoperacion=operaciones.id WHERE seccionesaoperar.idoperacion=$P{IDOPERACION}]]>
		</queryString>
		<field name="zona" class="java.lang.String"/>
		<field name="nombre" class="java.lang.String"/>
	</subDataset>
	<parameter name="IDOPERACION" class="java.lang.String">
