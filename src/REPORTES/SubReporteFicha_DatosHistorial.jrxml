		<queryString>
			<![CDATA[SELECT afecciones.nombre FROM afecciones INNER JOIN afeccionesxhistorialclinico ON afecciones.id=afeccionesxhistorialclinico.idafeccion INNER JOIN historialclinico ON afeccionesxhistorialclinico.idhistorial=historialclinico.id INNER JOIN fichamedica ON historialclinico.idficha=fichamedica.id INNER JOIN propietarios ON fichamedica.idpropietario=propietarios.id WHERE dni=$P{DNI} AND fichamedica.id=$P{IDFICHA}]]>
		</queryString>
		<field name="nombre" class="java.lang.String"/>
	</subDataset>
