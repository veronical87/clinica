		</parameter>
		<queryString>
			<![CDATA[SELECT parasitos FROM historialclinico INNER JOIN fichamedica ON historialclinico.idficha=fichamedica.id  WHERE historialclinico.idficha=$P{IDFICHA}]]>
		</queryString>
		<field name="parasitos" class="java.lang.String"/>
	</subDataset>
