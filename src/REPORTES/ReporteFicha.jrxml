	<queryString>
		<![CDATA[SELECT logoclinica.nombre AS CLINICA,localidades.nombre AS localidad,domicilio,CONCAT(codigo,'-',numerotelefono) AS numero,correo,logoclinica.direimagen,fichamedica.id AS NROFICHA,mascota,CONCAT(apellido,coma,propietarios.nombre) AS dueño,dni,edad,tiemponac,situacionpeso,SEXO,especies.nombre AS ESPECIE,kilaje,razas.nombre AS RAZA,fichamedica.direimagen AS imagenMASCOTA FROM localidades INNER JOIN logoclinica ON localidades.id=logoclinica.idlocalidad INNER JOIN fichamedica ON logoclinica.id=fichamedica.idlogo INNER JOIN propietarios ON propietarios.id=fichamedica.idpropietario INNER JOIN pelajexraza ON fichamedica.idpelaje=pelajexraza.id INNER JOIN razas ON pelajexraza.idraza=razas.id INNER JOIN especies ON razas.idespecie=especies.id WHERE dni=$P{DNI}]]>
